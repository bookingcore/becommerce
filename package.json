{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "f-dev": "cross-env process.env.section=frontend NODE_ENV=development mix",
        "f-watch": "npm run f-dev watch",
        "f-prod": "cross-env process.env.section=frontend NODE_ENV=production mix --production"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "cross-env": "^5.2.1",
        "jquery": "^3.5.1",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.4.5",
        "sass": "^1.23.0",
        "sass-loader": "^7.3.1",
        "tailwindcss": "^3.0.2",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.4.0",
        "@tinymce/tinymce-vue": "^3.2.1",
        "font-awesome": "^4.7.0",
        "he-tree-vue": "^1.1.4",
        "ionicons": "^4.6.3",
        "raw-loader": "^2.0.0",
        "style-loader": "^0.23.1",
        "vue-draggable-nested-tree": "^2.2.20",
        "vue-form-generator": "^2.3.4",
        "vuedraggable": "^2.23.2"
    }
}
